    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "12"
  },
  "main": "index.js",
  "dependencies": {
    "cors": "2.8.5",
    "cranny": "2.1.4",
    "ethers": "5.6.3",
    "firebase-admin": "10.0.2",
    "firebase-functions": "3.20.1",
    "google-cloud-secrets": "1.3.2",
    "google-cloud-report-error": "1.0.0",
    "superagent": "7.1.2"
  },
  "devDependencies": {
    "eslint": "7.21.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-promise": "4.3.1",
    "firebase-functions-test": "0.2.3"
  },
  "private": true
}
